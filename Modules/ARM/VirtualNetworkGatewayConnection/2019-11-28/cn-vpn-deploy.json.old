{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "vpnGWName": {
      "type": "string",
      "defaultValue": null,
      "metadata": {
        "description": "Arbitrary name for the azure gateway"
      }
    },
    "vpnGWConnName": {
      "type": "string",
      "defaultValue": null,
      "metadata": {
        "description": "Arbitrary name for the new connection between Azure VNet and other network"
      }
    },
    "vpnGWSharedKey": {
      "type": "string",
      "metadata": {
        "description": "Shared key (PSK) for IPSec tunnel"
      }
    },
    "vpnGWLocalGatewayName": {
      "type": "string",
      "defaultValue": "localGateway",
      "metadata": {
        "description": "Aribtary name for gateway resource representing customer on-premise site"
      }
    },
    "vpnGWUsePolicyBasedTrafficSelectors": {
      "type": "bool",
      "metadata": {
        "description": "saLifeTimeSeconds for connection"
      }
    },
    "vpnGWsaLifeTimeSeconds": {
      "type": "int",
      "metadata": {
        "description": "saLifeTimeSeconds for connection"
      }
    },
    "vpnGWsaDataSizeKilobytes": {
      "type": "int",
      "metadata": {
        "description": ""
      }
    },
    "vpnGWipsecEncryption": {
      "type": "string",
      "metadata": {
        "description": ""
      }
    },
    "vpnGWipsecIntegrity": {
      "type": "string",
      "metadata": {
        "description": ""
      }
    },
    "vpnGWikeEncryption": {
      "type": "string",
      "metadata": {
        "description": ""
      }
    },
    "vpnGWikeIntegrity": {
      "type": "string",
      "metadata": {
        "description": ""
      }
    },
    "vpnGWdhGroup": {
      "type": "string",
      "metadata": {
        "description": ""
      }
    },
    "vpnGWpfsGroup": {
      "type": "string",
      "metadata": {
        "description": ""
      }
    }
  },
  "variables": {
    "routingWeight": 3

  },
  "resources": [
    {
      "apiVersion": "2019-04-01",
      "name": "[parameters('vpnGWConnName')]",
      "type": "Microsoft.Network/connections",
      "location": "[resourceGroup().location]",
      "properties": {
        "virtualNetworkGateway1": {
          "id": "[resourceId('Microsoft.Network/virtualNetworkGateways', parameters('vpnGWName'))]"
        },
        "localNetworkGateway2": {
          "id": "[resourceId('Microsoft.Network/localNetworkGateways', parameters('vpnGWLocalGatewayName'))]"
        },
        "connectionType": "IPsec",
        "routingWeight": "[variables('routingWeight')]",
        "sharedKey": "[parameters('vpnGWSharedKey')]",
        "usePolicyBasedTrafficSelectors": "[parameters('vpnGWUsePolicyBasedTrafficSelectors')]",
        "ipsecPolicies": [
          {
            "saLifeTimeSeconds": "[parameters('vpnGWsaLifeTimeSeconds')]",
            "saDataSizeKilobytes": "[parameters('vpnGWsaDataSizeKilobytes')]",
            "ipsecEncryption": "[parameters('vpnGWipsecEncryption')]",
            "ipsecIntegrity": "[parameters('vpnGWipsecIntegrity')]",
            "ikeEncryption": "[parameters('vpnGWikeEncryption')]",
            "ikeIntegrity": "[parameters('vpnGWikeIntegrity')]",
            "dhGroup": "[parameters('vpnGWdhGroup')]",
            "pfsGroup": "[parameters('vpnGWpfsGroup')]"
          }
        ]
      }
    }
  ],
  "outputs": {
  }
}