{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "integrationServiceEnvironmentName": {
      "type": "string",
      "metadata": {
        "description": "Required. Name of the workflow."
      }
    },
    "integrationServiceEnvironmentLocation": {
      "type": "string",
      "defaultValue": "[resourceGroup().location]",
      "metadata": {
        "description": "Optional. Location for all resources."
      }
    },
    "skuName": {
      "type": "string",
      "metadata": {
        "description": "Required."
      }
    },
    "skuCapacity": {
      "type": "int",
      "defaultValue": 0,
      "metadata": {
        "description": "Required."
      }
    },
    "iseDelegatedVnetName": {
      "type": "string",
      "defaultValue": "vnet",
      "metadata": {
        "description": "Required."
      }
    },
    "iseDelegatedSubnetName": {
      "type": "string",
      "defaultValue": "subnet",
      "metadata": {
        "description": "Required"
      }
    },
    "iseDelegatedSubnetId": {
      "type": "string",
      "defaultValue": "",
      "metadata": {
        "description": "Required"
      }
    },
    "iseDelegatedSubnetAddressPrefix": {
      "type": "string",
      "defaultValue": "",
      "metadata": {
        "description": "Required"
      }
    },
    "subnetResourceGroup": {
      "type": "string",
      "defaultValue": "",
      "metadata": {
        "description": "Subnet's Resource Group"
      }
    },
    "subnetResourceIds": {
      "type": "array",
      "metadata": {
        "description": "Required."
      }
    },
    "accessEndpoint": {
      "type": "object",
      "metadata": {
        "description": "Required."
      }
    },
    "networkSecurityGroupId": {
      "type": "string",
      "defaultValue": ""
    },
    "routeTableId": {
      "type": "string",
      "defaultValue": ""
    },
    "serviceEndpoints": {
      "type": "array",
      "defaultValue": []
    },
    "tags": {
      "type": "object",
      "defaultValue": {},
      "metadata": {
        "description": "Optional. Tags of the Logic App resource."
      }
    }
  },
  "variables": {
    "integrationServiceEnvironmentDelegatedSubnet": {
      "name": "[concat(parameters('iseDelegatedVnetName'), '/', parameters('iseDelegatedSubnetName'))]",
      "id": "[parameters('iseDelegatedSubnetId')]",
      "properties": {
        "provisioningState": "Succeeded",
        "addressPrefix": "[parameters('iseDelegatedSubnetAddressPrefix')]",
        "serviceEndpoints": "[parameters('serviceEndpoints')]",
        "networkSecurityGroup": {
          "id": "[parameters('networkSecurityGroupId')]"
        },
        "routeTable": {
          "id": "[parameters('routeTableId')]"
        },
        "delegations": [
          {
            "name": "integrationServiceEnvironments",
            "properties": {
              "serviceName": "Microsoft.Logic/integrationServiceEnvironments",
              "actions": []
            }
          }
        ]
      },
      "type": "Microsoft.Network/virtualNetworks/subnets",
      "apiVersion": "2018-08-01"
    }
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2017-05-10",
      "name": "updateVirtualNetworkSubnetWithDelegation",
      "properties": {
        "mode": "Incremental",
        "parameters": {},
        "template": {
          "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "parameters": {},
          "variables": {},
          "resources": [
            "[variables('integrationServiceEnvironmentDelegatedSubnet')]"
          ]
        }
      },
      "resourceGroup": "[parameters('subnetResourceGroup')]"
    },
    {
      "type": "Microsoft.Logic/integrationServiceEnvironments",
      "apiVersion": "2018-07-01-preview",
      "name": "[parameters('integrationServiceEnvironmentName')]",
      "location": "[parameters('integrationServiceEnvironmentLocation')]",
      "dependsOn": [
        "Microsoft.Resources/deployments/updateVirtualNetworkSubnetWithDelegation"
      ],
      "tags": "[parameters('tags')]",
      "sku": {
        "name": "[parameters('skuName')]",
        "capacity": "[parameters('skuCapacity')]"
      },
      "properties": {
        "networkConfiguration": {
          "subnets": "[parameters('subnetResourceIds')]",
          "accessEndpoint": "[parameters('accessEndpoint')]"
        }
      }
    }
  ],
  "outputs": {},
  "functions": []
}