{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "wafName": {
            "value": "waf-cpp-apps-pre-we-01"
        },
        "location": {
            "value": "westeurope"
        },
        "applicationGateways": {
            "value": []
        },
        "requestBodyCheck": {
            "value": true
        },
        "maxRequestBodySizeInKb": {
            "value": 128
        },
        "fileUploadLimitInMb": {
            "value": 100
        },
        "state": {
            "value": "Enabled"
        },
        "mode": {
            "value": "Prevention"
        },
        "managedRuleSets": {
            "value": [
                {
                    "ruleSetType": "OWASP",
                    "ruleSetVersion": "3.0",
                    "ruleGroupOverrides": []
                }
            ]
        },
        "exclusions": {
            "value": [
                {
                    "matchVariable": "RequestCookieNames",
                    "selectorMatchOperator": "StartsWith",
                    "selector": "auth_tkt"
                }
            ]
        },
        "tags": {
            "value": {
                "Environment": "Pre"
            }
        }
    }
}