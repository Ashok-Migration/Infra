      "location": "westeurope",
      "sku": {
        "name": "DW100c",
        "capacity": 0
      },
      "properties": {
        "status": "Online",
        "maxSizeBytes": 263882790666240,
        "collation": "SQL_Latin1_General_CP1_CI_AS",
        "restorePointInTime": "0001-01-01T00:00:00",
        "creationDate": "2020-12-22T07:38:03.423Z",
        "provisioningState": "Succeeded"
      }
    },

    {
      "type": "Microsoft.Synapse/workspaces/sqlPools/auditingSettings",
      "apiVersion": "2019-06-01-preview",
      "name": "[concat(parameters('workspaces_name'), '/',parameters('pool_name'),'/Default')]",
      "dependsOn": [
        "[resourceId('Microsoft.Synapse/workspaces/sqlPools', parameters('workspaces_name'), parameters('pool_name'))]"
